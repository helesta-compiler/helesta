 
int exgcd2(int a,int b,int x[],int y[]) {
	if(b == 0) {
		return a;			
	}
	else {
		x[0] = 0;
		return a % b;
	}
} 
int exgcd(int a,int b,int x[],int y[]) {
	if(b == 0) {
		return a;			
	}
	else {
		int r = exgcd2(b, a % b, x, y);		
		x[0] = 0;
		return r;
	}
}
int main() {
	int a = 1, b = 1, x[1] = {1}, y[1] = {1};
	exgcd(a, b, x, y);
	return 0;
}
